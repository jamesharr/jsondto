<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='jsondto'>
  <source path='client'/>
  <source path='shared'/>
  <source path='generated'/>
  <inherits name='com.google.gwt.user.User'/>
  <inherits name="com.google.gwt.junit.JUnit"/>
  <inherits name="com.google.gwt.json.JSON"/>
  <generate-with class="org.grickle.rebind.PicklerGeneratorGWT">
    <when-type-assignable class="org.grickle.client.Pickler"/>
  </generate-with>
  <generate-with class="org.grickle.rebind.RpcServiceGenerator">
    <when-type-assignable class="org.grickle.client.JsonRpcService"/>
  </generate-with>
</module>
